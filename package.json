    "lint:all": "nx run-many --target=lint",
    "gauntlet": "npm run prettier && npm run lint:all -- --fix && npm run test:all && npm run merge-code-coverage && npm run build:all && npm run lint:all",
    "gauntlet:6": "npm run prettier && npm run lint:all -- --fix --parallel=6 && npm run test:all -- --parallel=6 && npm run merge-code-coverage && npm run build:all -- --parallel=6 && npm run lint:all -- --parallel=6"
