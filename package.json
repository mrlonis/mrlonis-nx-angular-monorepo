{
  "name": "mrlonis-nx-angular-monorepo",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx lint",
    "prettier": "prettier --config .prettierrc --write \"**/*.{ts,js,css,scss,html,json,md}\"",
    "prepare": "husky install",
    "build:all": "nx run-many --target=build",
    "test:all": "nx run-many --target=test",
    "lint:all": "nx run-many --target=lint",
    "gauntlet": "npm run build:all && npm run test:all && npm run lint:all",
    "repair": "nx repair"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.1",
    "@angular/cdk": "^14.2.2",
    "@angular/common": "^14.2.1",
    "@angular/compiler": "^14.2.1",
    "@angular/core": "^14.2.1",
    "@angular/forms": "^14.2.1",
    "@angular/material": "^14.2.2",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "@angular/router": "^14.2.1",
    "@nrwl/angular": "14.7.5",
    "rxjs": "^7.5.6",
    "tslib": "^2.4.0",
    "zone.js": "^0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular-eslint/eslint-plugin": "^14.0.4",
    "@angular-eslint/eslint-plugin-template": "^14.0.4",
    "@angular-eslint/template-parser": "^14.0.4",
    "@angular/cli": "^14.2.2",
    "@angular/compiler-cli": "^14.2.1",
    "@angular/language-service": "^14.2.1",
    "@nrwl/cli": "14.7.5",
    "@nrwl/cypress": "14.7.5",
    "@nrwl/eslint-plugin-nx": "14.7.5",
    "@nrwl/jest": "14.7.5",
    "@nrwl/linter": "14.7.5",
    "@nrwl/workspace": "14.7.5",
    "@types/jest": "^28.1.1",
    "@types/node": "^16.11.59",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "cypress": "^10.8.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "jest-preset-angular": "^12.2.2",
    "lint-staged": "^13.0.3",
    "ng-lint-staged": "^12.0.4",
    "nx": "14.7.5",
    "prettier": "^2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.9.1",
    "typescript": "~4.8.3"
  },
  "lint-staged": {
    "*.{css,html,js,jsx,json,md,ts,tsx,scss}": [
      "prettier --write"
    ],
    "apps/ui-afk-arena/**/*.{html,js,jsx,ts,tsx}": [
      "prettier --write",
      "ng-lint-staged lint ui-afk-arena --fix --"
    ],
    "apps/ui-afk-arena-e2e/**/*.{html,js,jsx,ts,tsx}": [
      "prettier --write",
      "ng-lint-staged lint ui-afk-arena-e2e --fix --"
    ],
    "apps/ui-genshin-impact/**/*.{html,js,jsx,ts,tsx}": [
      "prettier --write",
      "ng-lint-staged lint ui-genshin-impact --fix --"
    ],
    "apps/ui-genshin-impact-e2e/**/*.{html,js,jsx,ts,tsx}": [
      "prettier --write",
      "ng-lint-staged lint ui-genshin-impact-e2e --fix --"
    ],
    "apps/ui-mythic-heroes/**/*.{html,js,jsx,ts,tsx}": [
      "prettier --write",
      "ng-lint-staged lint ui-mythic-heroes --fix --"
    ],
    "apps/ui-mythic-heroes-e2e/**/*.{html,js,jsx,ts,tsx}": [
      "prettier --write",
      "ng-lint-staged lint ui-mythic-heroes-e2e --fix --"
    ],
    "apps/ui-personal-website/**/*.{html,js,jsx,ts,tsx}": [
      "prettier --write",
      "ng-lint-staged lint ui-personal-website --fix --"
    ],
    "apps/ui-personal-website-e2e/**/*.{html,js,jsx,ts,tsx}": [
      "prettier --write",
      "ng-lint-staged lint ui-personal-website-e2e --fix --"
    ],
    "libs/ngx-mrlonis-shared/**/*.{html,js,jsx,ts,tsx}": [
      "prettier --write",
      "ng-lint-staged lint ngx-mrlonis-shared --fix --"
    ]
  }
}
