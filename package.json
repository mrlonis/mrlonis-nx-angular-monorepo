  "scripts": {
    "affected": "nx affected",
    "build": "nx build",
    "build:all": "nx run-many -t build",
    "e2e:all": "nx run-many -t e2e",
    "format:check": "nx format:check",
    "format:write": "nx format:write",
    "gauntlet": "npm run gauntlet:format && npm run gauntlet:test",
    "gauntlet:e2e": "npm run gauntlet && npm run e2e:all",
    "gauntlet:format": "npm run prettier && npm run format:write && npm run format:check",
    "gauntlet:test": "npm run lint:all -- --fix && npm run lint:all && npm run test:all -- --configuration ci && npm run build:all",
    "graph": "nx graph",
    "postinstall": "node ./decorate-angular-cli.js",
    "lint": "nx lint",
    "lint:all": "nx run-many -t lint",
    "ng": "nx",
    "nx": "nx",
    "prepare": "husky install",
    "prettier": "prettier --config .prettierrc --write \"**/*.{css,html,js,jsx,json,md,scss,ts,tsx}\"",
    "prettier:test": "prettier --config .prettierrc --list-different \"**/*.{css,html,js,jsx,json,md,scss,ts,tsx}\"",
    "repair": "nx repair",
    "reset": "nx reset",
    "sort": "npm run sort-package-json && npm run sort-vscode-settings-json",
    "sort-package-json": "sort-package-json && prettier --config .prettierrc --write package.json",
    "sort-vscode-settings-json": "sort-json --indent 2 --depth 4 .vscode/settings.json && prettier --config .prettierrc --write .vscode/settings.json",
    "start": "nx serve",
    "test": "nx test",
    "test:all": "nx run-many -t test"
  },
