{
  "cSpell.words": [
    "animejs",
    "Buildable",
    "cacheable",
    "cobertura",
    "compodoc",
    "fizzbuzz",
    "frontends",
    "genshin",
    "lintstagedrc",
    "Lonis",
    "Mrlonis",
    "nestjs",
    "nrwl",
    "packagr",
    "prettierrc",
    "subdir",
    "todos"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.organizeImports": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "eslint.options": {
    "cache": true,
    "fix": true
  },
  "eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact", "html", "json"],
  "files.eol": "\n"
}
