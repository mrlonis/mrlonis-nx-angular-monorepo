{
  "name": "interview-angular-karma-workspace",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/interview-angular-karma-workspace/src",
  "prefix": "mrlonis",
  "targets": {
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "main": "libs/interview-angular-karma-workspace/src/test.ts",
        "tsConfig": "libs/interview-angular-karma-workspace/tsconfig.spec.json",
        "karmaConfig": "libs/interview-angular-karma-workspace/karma.conf.js"
      },
      "configurations": {
        "ci": {
          "watch": false,
          "browsers": "ChromeHeadless",
          "codeCoverage": true,
          "reporters": ["progress", "coverage", "junit"]
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/interview-angular-karma-workspace/**/*.ts",
          "libs/interview-angular-karma-workspace/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
